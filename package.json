{
	"name": "my-app",
	"version": "0.0.1",
	"private": true,
	"author": {
		"name": "Francesco Bruno"
	},
	"scripts": {
		"dev": "npm-run-all --parallel vite typesafe-i18n",
		"vite": "vite dev --port 4200",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin=prettier-plugin-svelte --plugin=prettier-plugin-organize-imports --plugin=prettier-plugin-tailwindcss --write .",
		"typesafe-i18n": "typesafe-i18n"
	},
	"devDependencies": {
		"@melt-ui/pp": "^0.1.2",
		"@melt-ui/svelte": "^0.52.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"autoprefixer": "^10.4.15",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"mdsvex": "^0.11.0",
		"postcss": "^8.4.29",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"svelte-sequential-preprocessor": "^2.0.1",
		"tailwindcss": "^3.3.3",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"type": "module",
	"dependencies": {
		"@unovis/svelte": "^1.2.1",
		"@unovis/ts": "^1.2.1",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.0.0",
		"lucide-svelte": "^0.284.0",
		"npm-run-all": "^4.1.5",
		"prettier-plugin-organize-imports": "^3.2.3",
		"prettier-plugin-svelte": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.5.5",
		"tailwind-merge": "^1.14.0",
		"tailwindcss-animate": "^1.0.7",
		"typesafe-i18n": "^5.26.2"
	}
}
